pipeline {
	agent any 
			stages {
				stage('One') {
					steps {
						echo "Hi, this is Kundan from Evry"
						dotnet restore
					}
				}
				
				stage('Two') {
					steps {
						input('Do you want to proceed?')
						dotnet msbuild api.sln
					}
				}
				
				stage ('Three') {
					steps {
						echo "Processing further..."
						dotnet publish
					}
				}
				
				stage ('Four') {
					steps {
						dotnet test api.Tests/api.Tests.csproj
						dotnet test "--logger:trx;LogFileName=Results.trx"
					}
				}
			}
		}